{
  "name": "googleshokry/glib",
  "description": "glib",
  "keywords": [
    "laravel",
    "glib"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Googleshokry",
      "email": "sms.shokry.mohamed@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "GoogleShokry\\Glib\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoogleShokry\\Glib\\Test\\": "tests/"
    }
  },
  "minimum-stability":"dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Glib\\GlibServiceProvider"
      ]
    }
  }
}
